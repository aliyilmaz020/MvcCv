@model MvcCv.Models.Entity.Admin
@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<title>Admin Güncelleme</title>
<br />
<h3>Admin Güncelleme Sayfası</h3>
<br />
<form method="post">
    <input type="hidden" name="ID" class="form-control" value="@Model.ID" readonly />
    <br />
    <input type="text" name="Username" class="form-control" value="@Model.Username" required />
    <br />
    <input type="text" name="Password" class="form-control" value="@Model.Password" required />
    <br />
    <button class="btn btn-info" type="submit">Güncelle</button>
</form>

